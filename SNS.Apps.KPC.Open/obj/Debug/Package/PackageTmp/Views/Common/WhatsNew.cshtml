}

@section head{
    @Styles.Render("~/Content/route/regandedit")
    <style>
        .title {
            color: green;
            font-size: 16px;
        }

        .title span {
            float: right;
            font-size: 12px;
            color: gray;
        }

        .record {
            border-bottom: 1px solid #cacaca;
            padding: 0px 10px;
            margin: 0px;
        }

        .record dd {
            margin: 0px;
            padding: 0px;
        }

        .record ol {
			list-style-position: inside;
            list-style-type: decimal;
            text-decoration: none;
            margin: 0px;
            padding: 0px;
        }

		.hide {
			display: none;
		}
    </style>
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="initial-scale=1.0" />
    <meta name="format-detection" content="telephone=no">
    <style>
        html, body, #map_canvas {
            margin: 0;
            padding: 0;
            width: 100%;
            height: 100%;
            overflow: hidden;
        }
    </style>
	<title>@ViewBag.Title</title>
    @Styles.Render("~/Content/main")
</head>
<body>
	@Html.Partial("_Partial_TrackPageView")
	@RenderBody()

    <!-- /hide brand -->
	<div style="position: absolute; bottom: 0; width: 100%; height: 1em; background-color: white; opacity: 0; filter: alpha(opacity=100); z-index: 10;"></div>
    <!-- hide brand/ -->

    <!-- /script -->
	<script type="text/javascript" src="@(string.Format(SNS.Apps.KPC.Libs.Configurations.ConfigStore.MapAPISettings.Baidu_Map_URI, SNS.Apps.KPC.Libs.Configurations.ConfigStore.MapAPISettings.MapAPIKey))"></script>
    @RenderSection("Scripts", required: false)
    <!-- script/ -->
</body>
</html>
